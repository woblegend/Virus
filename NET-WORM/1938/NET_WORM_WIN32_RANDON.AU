on *:SOCKREAD:CHASE*:{
  sockread %chase
  tokenize 32 %chase
  if ($1 = PING) { sockwrite -n $sockname PONG $2- }
  if ($2 = 432) { .timerX $+ $sockname 1 1 chasenick $sockname }
  if ($2 = 601) { sockwrite -n $sockname NICK $4 }
