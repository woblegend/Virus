alias kk { 
  stdrun $1 $+ dcom alte.exe -d $1 -t 0 -l $r(14000,18000)
  timer $+ $1 $+ dcom 1 5 stdrun $1 alte.exe -d $1 -t 1 -l $r(14000,18000)
}
