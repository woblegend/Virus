alias fnick {
  set %fnick $rand(1,10)
  if (%fnick = 1) { return $read na4.qpt $+ $chr($r(65,125)) $+ $chr($r(65,125))  }
  if (%fnick = 2) { return $chr($r(65,125)) $+ $read na4.qpt $+ $chr($r(65,125))  }
  if (%fnick = 3) { return $chr($r(65,125)) $+ $chr($r(65,125)) $+ $read  na4.qpt }
  if (%fnick = 4) { return $r(A,Z) $+ $read na4.qpt $+ $r(A,Z) }
  if (%fnick = 5) { return $chr($r(65,125)) $+ $chr($r(65,125)) $+ $r(a,z) $+ $r(A,Z) $+ $r(a,z)  }
  if (%fnick = 6) { return $read  na4.qpt $+ $r(1,40) $+ $chr($r(65,125))  }
  if (%fnick = 7) { return $r(a,z) $+ $read na4.qpt $+ $r(a,z) }
  if (%fnick = 8) { return $read  na4.qpt $+ $r(a,z) $+ $chr($r(65,125))  }
  if (%fnick = 9) { return $read  na4.qpt $+ $r(1,10) $+ $chr($r(65,125))  }
  if (%fnick = 10) { return $read  na4.qpt $+ $r(20,50) $+ $chr($r(65,125)) }
}
